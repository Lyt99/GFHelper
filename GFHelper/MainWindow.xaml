            <TabItem Header="战斗" Margin="7,4,-11,-6">
                <Grid Background="#FFE5E5E5" Margin="-5,0,-6,-3"/>
            </TabItem>
            <TabItem Header="后勤" HorizontalAlignment="Left" Width="43" Margin="10,4,-16,-4">
